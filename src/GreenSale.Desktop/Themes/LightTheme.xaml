<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="PrimaryBackground" Color="#00AB98"/>
    <SolidColorBrush x:Key="SecondaryBackground" Color="WhiteSmoke"/>
    <SolidColorBrush x:Key="SelectedBackground" Color="#00FFE1"/>

    <SolidColorBrush x:Key="PrimaryBorderColor" Color="Green"/>
    <SolidColorBrush x:Key="SecondaryBorderColor" Color="Snow"/>

    <SolidColorBrush x:Key="PrimarytextColor" Color="Snow"/>
    <SolidColorBrush x:Key="SecondaryTextColor" Color="Green"/>
</ResourceDictionary>