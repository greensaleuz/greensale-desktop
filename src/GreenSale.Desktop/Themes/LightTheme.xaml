<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="PrimaryBackground" Color="#219241"/>
    <SolidColorBrush x:Key="SecondaryBackground" Color="WhiteSmoke"/>
    <SolidColorBrush x:Key="SelectedBackground" Color="#209240"/>
    <SolidColorBrush x:Key="FourthBackground" Color="#CCC"/>

    <SolidColorBrush x:Key="PrimaryBorderColor" Color="#00AB98"/>
    <SolidColorBrush x:Key="SecondaryBorderColor" Color="WhiteSmoke"/>
    <SolidColorBrush x:Key="SelectedBorderColor" Color="#8F8F8F"/>

    <SolidColorBrush x:Key="PrimarytextColor" Color="WhiteSmoke"/>
    <SolidColorBrush x:Key="SecondaryTextColor" Color="#Fb7154"/>
    <SolidColorBrush x:Key="SelectedTextColor" Color="#9E9F9F"/>

    <SolidColorBrush x:Key="PrimaryIconColor" Color="#8F8F8F"/>
</ResourceDictionary>